{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "routes": [
    {
      "src": "^/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/static/$1"
    },
    {
      "src": "^/_next/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/_next/static/$1"
    },
    {
      "src": "^/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/join/(default|signup)/?$",
      "dest": "/join/$1/index.html"
    },
    {
      "src": "^/(dashboard|settings|onboarding|privacy|terms|contact)/?$",
      "dest": "/$1/index.html"
    },
    {
      "src": "^/404/?$",
      "dest": "/404.html"
    },
    {
      "src": "/((?!.*\\..*$).*)",
      "dest": "/index.html"
    }
  ]
} 